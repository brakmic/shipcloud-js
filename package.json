{
  "name": "shipcloud-js",
  "private": true,
  "version": "0.0.1",
  "description": "ShipCloud API for JS",
  "main": "index.js",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server -- --client-log-level none",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:client": "npm run build:dev -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "api:dev": "webpack-dev-server --config config/webpack.api.dev.js --progress --profile --watch --content-base src/",
    "api:dev:hot": "npm run api:dev -- --inline --hot",
    "build:dev": "webpack --config config/webpack.api.dev.js --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.api.prod.js  --progress --profile --bail",
    "dashboard": "webpack-dashboard -- npm run server:dev:hmr",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run build:prod",
    "start:hmr": "npm run api:dev:hot",
    "start:client": "ts-node index.ts",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "Harris Brakmic",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.5.41",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.4",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.4",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "bootstrap-loader": "^2.0.0-beta.20",
    "circular-json": "^0.3.1",
    "codelyzer": "^2.0.0-beta.4",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "diff": "^3.2.0",
    "domready": "^1.0.8",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "font-awesome-loader": "^1.0.2",
    "imports-loader": "^0.7.0",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "moment": "^2.17.1",
    "node-fetch": "^1.6.3",
    "node-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "script-ext-html-webpack-plugin": "^1.5.0",
    "source-map-loader": "^0.1.6",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5",
    "typescript-collections": "^1.2.3",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^1.16.3",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "js-base": "0.0.1-beta",
    "lodash": "^4.17.4",
    "rxjs": "^5.1.0"
  }
}
